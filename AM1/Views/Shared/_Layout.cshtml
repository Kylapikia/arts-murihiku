<!DOCTYPE html>
<html>
<head>

    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] </title>
    <script src="//code.jquery.com/jquery-2.1.4.min.js"></script>
    <link rel="shortcut icon" type="image/x-icon" href="~/images/favicon.ico" />
    <link type="text/css" rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.rawgit.com/dmuy/MDTimePicker/7d5488f/mdtimepicker.min.css">
    <environment include="Development">
        <link rel="stylesheet" href="~/css/Version6/bootstrap.css" />
        <link rel="stylesheet" href="~/css/Version6/site.css" />
        <link rel="stylesheet" href="~/css/Shop.css" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel='stylesheet' href='~/css/Kyla.css' />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/css/bootstrap.min.css"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
    <script src='https://www.google.com/recaptcha/api.js'></script>
    <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a asp-area="" asp-controller="Home" asp-action="Index" class="navbar-brand"><img style="max-width:150px; margin-top: -7px;" src="~/images/foot lolgo - Copy.png" class="Icon"></a>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-center">
                    <li class="active NavText"><a asp-area="" asp-controller="Home" asp-action="Index">Introduction</a></li>
                    <li class="active NavText"><a asp-area="" asp-controller="Home" asp-action="Directory">Artist Directory</a></li>
                    <li class="active NavText"><a asp-area="" asp-controller="Home" asp-action="Events">Events</a></li>
                    <li class="active NavText"><a asp-area="" asp-controller="Home" asp-action="Trail">Art trail</a></li>
                </ul>

                @await Html.PartialAsync("_LoginPartial")
            </div>
        </div>

    </nav>

    <div class="container body-content">
        @RenderBody()
        
        <div id="myNav" class="overlay ">

            <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
            <div class="overlay-content">
                <div class="col-md-9 vl text-right">
                    <a asp-area="" asp-controller="Home" asp-action="Index">Introduction</a>
                    <a asp-area="" asp-controller="Home" asp-action="About">About</a>
                    <a asp-area="" asp-controller="Home" asp-action="Team">Meet the team</a>
                    <a asp-area="" asp-controller="Blogs" asp-action="Index">Blog</a>
                    <a asp-area="" asp-controller="Products" asp-action="Index">Shop</a>
                    <a asp-area="" asp-controller="VolForms" asp-action="Create">Volunteer</a>
                    <a asp-area="" asp-controller="Home" asp-action="Contact">Contact us</a>
                </div>

                <div class="col-md-3 text-left ">
                    <br /><br /><br /><br /><br /><br /><br /><br /><br /><br /><br />
                    <a href="https://www.facebook.com/artsmurihiku/" target="_blank">Facebook</a>
                    <a href="https://www.instagram.com/artsmurihiku/" target="_blank">Instagram</a>

                </div>

            </div>
        </div>


        <div class="translucy block">
            <footer data-aos="fade-up">
                <div data-aos="fade-up" class="container">
                    <div class="col-md-3 ">

                        <br />
                        <img height="100" width="280" src="~/images/foot lolgo.png" />
                    </div>
                    <div class="col-md-3 noOverlap">



                        <h4>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;    Contact us</h4>
                        <p>
                            &nbsp; &nbsp; &nbsp; &nbsp; Community Trust of Southland,<br>
                            &nbsp; &nbsp; &nbsp; &nbsp; 62 Don Street, Invercargill 9840 <br>
                            &nbsp; &nbsp; &nbsp; &nbsp; PO Box 1646, Invercargill, 9840 <br>
                        </p>
                        <p>&nbsp; &nbsp; &nbsp; &nbsp; Phone: <a href="tel:+64 21 055 6674">+64 21 055 6674</a></p>


                        <a href="~/Views/"></a>
                    </div>
                    <div class="col-md-3">
                        <h4>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; Stay Connected</h4>
                        <a href="https://www.facebook.com/artsmurihiku/" target="_blank"> <img class="Social" src="~/images/insta.jpg"></a>
                        <a href="https://www.instagram.com/artsmurihiku/" target="_blank"><img class="Social" src="~/images/fb.jpg"></a>
                        <br /><br />
                        <strong class="text-center">&nbsp;&nbsp;&nbsp;Sign up for our email newsletter</strong>  <br />  <br />
                        <form><input type="email"> <button type="submit">Sign up</button></form>
                        <br />

                    </div>
                    <div class="col-md-3">
                        <h4>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp;  Associates</h4>
                        <a href="http://www.ctos.org.nz/" target="_blank"><img class="Associates" src="~/images/soutlandCOM.png"></a>
                        <a href="http://www.creativenz.govt.nz/" target="_blank">  <img class="Associates" src="~/images/creativeNZ.png"></a>
                    </div>
                    <br />
                    <br />

                    <fieldset><legend></legend></fieldset>
                    <div class="col-xs-12">

                        <p class="text-center "><a asp-area="" asp-controller="Home" asp-action="Contact">Contact us</a>  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a asp-area="" asp-controller="Home" asp-action="SiteMap">Sitemap</a>   &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;  <a asp-area="" asp-controller="Home" asp-action="PrivacyPolicy">Privacy Policy</a>    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <a asp-area="" asp-controller="Home" asp-action="TermsOfUse">Terms of Use</a>  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Arts Murihiku 2018© </p>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <environment include="Development">
        <script src="~/js/text.js"></script>
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
    </environment>
    <environment exclude="Development">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha384-K+ctZQ+LL8q6tP7I94W+qzQsfRV2a+AfHIi9k8z8l9ggpc8X+Ytst4yBo/hH+8Fk">
        </script>
        <script src="https://ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa">
        </script>
        <script src="~/js/site.min.js" asp-append-version="true"></script>
    </environment>
    <script src="https://cdn.rawgit.com/dmuy/MDTimePicker/7d5488f/mdtimepicker.min.js"></script>
    <script src='https://www.google.com/recaptcha/api.js'></script>
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
        AOS.init();

    </script>
    <script>
        function openNav() {
            document.getElementById("myNav").style.height = "100%";
        }

        function closeNav() {
            document.getElementById("myNav").style.height = "0%";
        }</script>
    @RenderSection("Scripts", required: false)
</body>
</html>
